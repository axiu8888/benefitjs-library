{
  "name": "pdf-creator",
  "version": "1.0.0",
  "main": "dist/electron/main/index.js",
  "description": "生产PDF",
  "author": "dingxiuan <dingxiuan@163.com>",
  "license": "MIT",
  "private": true,
  "keywords": [
    "electron",
    "rollup",
    "vite",
    "vue3",
    "vue"
  ],
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344/"
    }
  },
  "type": "module",
  "scripts": {
    "dev-win": "chcp 65001 && vite",
    "dev-mac": "vite",
    "build-vue": "vue-tsc --noEmit && vite build",
    "build": "npm run build-vue && vite build && electron-builder",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit",
    "build:linux": "electron-packager . pdf-creator --platform=linux --arch=x64 --out ./install/ --overwrite --app-version=1.0.0 --electron-version=33.2.1 --icon=./logo.ico --asar",
    "build:win_x32": "electron-packager . pdf-creator --win32 --arch=ia32 --out ./install/ --overwrite --app-version=1.0.0 --electron-version=33.2.1 --icon=./logo.ico --asar",
    "build:win_x64": "electron-packager . pdf-creator --win32 --arch=x64 --out ./install/ --overwrite --app-version=1.0.0 --electron-version=33.2.1 --icon=./logo.ico --asar",
    "ignore": "~"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.4",
    "electron": "^33.2.1",
    "electron-builder": "^24.13.3",
    "electron-packager": "^17.1.2",
    "typescript": "^5.4.2",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.1.5",
    "vite-plugin-electron": "^0.28.4",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vue": "^3.4.31",
    "vue-tsc": "^2.0.22"
  },
  "dependencies": {
    "@benefitjs/core": "workspace:^",
    "@benefitjs/node": "workspace:^",
    "ant-design-vue": "^4.1.2",
    "axios": "^1.6.8",
    "commander": "^12.0.0",
    "less": "^4.2.0",
    "rw": "^1.3.3"
  }
}
