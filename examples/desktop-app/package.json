{
  "name": "desktop-app",
  "version": "0.1.0",
  "main": "dist/electron/main/index.js",
  "description": "桌面程序",
  "author": "dingxiuan",
  "license": "MIT",
  "private": true,
  "keywords": [
    "electron",
    "rollup",
    "vite",
    "vue3",
    "vue"
  ],
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344/"
