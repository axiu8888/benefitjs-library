    "@benefitjs/core": "workspace:^",
    "@benefitjs/devices": "workspace:^",
    "@benefitjs/widgets": "workspace:^",
    "ant-design-vue": "^4.0.7",
    "axios": "^1.5.0",
    "bootstrap-icons": "^1.11.2",
